        "Ф(Т)Ж | VF": "{base_rhythm} && (vent) && (fib || flutter)"
    },
    "Эктопические комплексы": {
        "НЖЭ | SP": " ({ectopic} && (atrial) && (premature || escape)) || ({ectopic} && (nodal_AV) && (premature || escape))",
        "ЖЭ | VP": "{ectopic} && (vent) && (premature || escape)",
        "Алоритмии | Al": " ({ectopic} && (bi)) || ({ectopic} && (tri)) || ({ectopic} && (quadri))"
